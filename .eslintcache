uctdata.map((product) => (\r\n                  <Box key={product.id} ml={1} mb={1}>\r\n                    <ProductItem onAddClick={onAddClick} product={product} />\r\n                  </Box>\r\n                ))}\r\n              </Box>\r\n              <Box display=\"flex\" mt={2} justifyContent=\"center\" alignItems=\"center\">\r\n                <Pagination\r\n                  count={totalPages}\r\n                  page={_page}\r\n                  onChange={handlePageChange}\r\n                  siblingCount={1}\r\n                  boundaryCount={2}\r\n                  variant=\"outlined\"\r\n                  color=\"primary\"\r\n                />\r\n              </Box>\r\n            </Box>\r\n            <Box width=\"30%\" mr={2}>\r\n              <Typography component=\"h5\" variant=\"h5\">\r\n                Sắp xếp sản phẩm\r\n              </Typography>\r\n              <FormControlLabel\r\n                control={\r\n                  <Switch\r\n                    checked={atoz}\r\n                    onChange={(e) => onChangeAtoZ && onChangeAtoZ(e.target.checked)}\r\n                    name=\"atoz\"\r\n                  />\r\n                }\r\n                label=\"A->Z\"\r\n              />\r\n              <FormControlLabel\r\n                control={\r\n                  <Switch\r\n                    checked={ztoa}\r\n                    onChange={(e) => onChangeZtoA && onChangeZtoA(e.target.checked)}\r\n                    name=\"ztoa\"\r\n                  />\r\n                }\r\n                label=\"Z->A\"\r\n              />\r\n              <FormControlLabel\r\n                control={\r\n                  <Switch\r\n                    checked={min}\r\n                    onChange={(e) => onChangePriceMin && onChangePriceMin(e.target.checked)}\r\n                    name=\"mintomax\"\r\n                  />\r\n                }\r\n                label=\"$ min -> $ max\"\r\n              />\r\n              <FormContr